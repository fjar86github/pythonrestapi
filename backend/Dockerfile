# Gunakan Python imageFROM python:3.9-slim# Set working directoryWORKDIR /app# Copy semua file backendCOPY . /app# Install dependenciesRUN pip install --no-cache-dir -r requirements.txt# Expose portsEXPOSE 5001 5002 5003 5004 5005 5006# Command untuk menjalankan semua app.pyCMD ["sh", "-c", "  python app.py & \  python app2.py & \  python app3.py & \  python app4.py & \  python app5.py & \  python app6.py && wait"]